<div class="page-title">
  <h1>Editing note</h1>
</div>
<div class="container-responsive-margin">
  <div class="form-container">
    <%= render "form", note: @note %>
  </div>
</div>
<div>
  <%= link_to "Show this note", @note, class: "btn btn-dark btn-sm nav-button" %>
  <%= button_to "Destroy this note",
  @note,
  method: :delete,
  class: "btn btn-dark btn-sm nav-button" %>
</div>
